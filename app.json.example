{
  "expo": {
    "name": "Confy App",
    "slug": "confy-app",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "extra": {
      "androidGoogleClientId": "<add here your Android's Google OAuth Client ID>",
      "iosGoogleClientId": "<add here your iOS's Google OAuth Client ID>"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}
